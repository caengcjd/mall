package mystruts;

/**
 * 页面跳转类
 * 
 * @author liuyong
 * 
 */
public class ActionForward {
	private String name;
	private String path;

	public ActionForward(String name, String path) {
		this.name = name;
		this.path = path;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getPath() {
		return path;
	}

	public void setPath(String path) {
		this.path = path;
	}
}
